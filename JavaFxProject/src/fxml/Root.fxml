<HBox xmlns:fx="http://javafx.com/fxml" fx:controller="fxml.RootController">
	<padding>
		<Insets topRightBottomLeft="10.0"/> 
		<!-- <Insets top="10.0" right="10.0" bottom="10.0" left="10.0" /> --> 
	</padding>
	<spacing> 10 </spacing>
	
	<children>
		<TextField fx:id="textField"prefWidth = "200" /> <!--여기서fx:id="??"는 변수 나중에 이 변수를 호출해서 사용할려고 씀 -->
		<Button fx:id"brnOk"text = "확인" />
	</children>
</HBox>
